{
  "Vendor": true,
  "Deadline": "2m",
  "Sort": ["linter", "severity", "path", "line"],
  "EnableGC": true,
  "WarnUnmatchedDirective": true,
  "Exclude": ["parameter .* always receives"],

  "DisableAll": true,
  "Enable": [
    "deadcode",
    "gofmt",
    "goimports",
    "golint",
    "gosimple",
    "ineffassign",
    "interfacer",
    "lll",
    "misspell",
    "unconvert",
    "unparam",
    "unused",
    "vet"
  ],

  "LineLength": 160
}
